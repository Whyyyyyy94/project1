<%= form_for @supplement do |f|%>
    <%=f.label :name%>
    <%=f.text_field :name%>

    <%=f.label :intakedirection%>
    <%=f.text_field :intakedirection%>

    <%=f.label :effects%>
    <%=f.text_field :effects%>

    <%= f.label :gymplan_id%>
    <%= f.collection_check_boxes :gymplan_ids, Gymplan.all, :id, :focus%>

    <%=f.submit%>
<%end%>
