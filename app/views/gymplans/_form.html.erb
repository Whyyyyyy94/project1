<%= form_for @gymplan do |f|%>
    <%= f.label :day, :class => 'form-label'%>
    <%= f.number_field :day, :class => 'form-control'%>

    <%= f.label :focus, :class => 'form-label'%>
    <%= f.text_field :focus, :class => 'form-control'%>

    <%= f.label :image, :class => 'form-label'%>
    <%= f.url_field :image, :class => 'form-control'%>
    </br>

    <%= f.label :supplement_id, :class => 'form-label'%>: 
    <%= f.collection_check_boxes :supplement_ids, Supplement.all, :id, :name, :class => 'form-control'%>
    </br>
    <%= f.submit :class => 'btn btn-success btn-lg'%>
<%end%>
