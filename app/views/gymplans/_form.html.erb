<%= form_for @gymplan do |f|%>
    <%= f.label :day%>
    <%= f.number_field :day%>

    <%= f.label :focus%>
    <%= f.text_field :focus%>

    <%= f.label :image%>
    <%= f.url_field :image%>

    <%= f.label :supplement_id%>
    <%= f.collection_check_boxes :supplement_ids, Supplement.all, :id, :name%>
    <%= f.submit%>
<%end%>
