<%= form_for @exercise do |f|%>
    <%= f.label :name%>
    <%= f.text_field :name%>

    <%= f.label :reps%>
    <%= f.number_field :reps%>

    <%= f.label :sets%>
    <%= f.number_field :sets%>

    <%= f.label :image%>
    <%= f.url_field :image%>

    <%= f.label :gymplan_id%>
    <%= f.select :gymplan_id, Gymplan.pluck(:focus, :id), :include_blank => true%>

    <%= f.submit%>
<%end%>
